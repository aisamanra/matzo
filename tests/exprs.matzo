(* catenation *)
puts This That The-Other;
(* choice *)
puts This | That | The-Other;
(* weighted choice *)
puts 5: This | That;

(* application *)
puts foo[bar][baz];


(* tuples *)
puts <>;
puts <1>;
puts <1,2>;
puts <1,2,3>;

(* ranges *)
puts 0..20;
puts x..y;

(* application *)
puts f[x];

(* lambdas *)
puts fn { [x] => x };
puts fn { [True] => "yes" ; [False] => "no" };