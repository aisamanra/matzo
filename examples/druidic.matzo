(* Druidic *)
word := init (rep.<1|2,syll>) end;
init := 3: vowel | 2: '';
syll := cons vowel;
end ::= a e er ir;
cons ::= b c c c d d f g h j k l m n p qu r
         s s s š š š t t t ts v w y z;
vowel ::= a e i o u â ä î ai au äu ea ei eu eai eau ia ie io
          iu ii oa oi ou uä ui;

puts word;
